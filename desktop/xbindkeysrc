"xbindkeys_show"
  Alt + k

# Increase volume
"pactl set-sink-volume @DEFAULT_SINK@ +1000"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-volume @DEFAULT_SINK@ -1000"
   XF86AudioLowerVolume

# Mute volume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

# Increase backlight
"xbacklight -inc 10"
   XF86MonBrightnessUp

# Decrease backlight
"xbacklight -dec 10"
   XF86MonBrightnessDown

"L comp -e "
  Alt + c

"L sig"
  Alt + i

"rofi -show drun"
  Alt + d

"9 9term"
  Alt + Return

"xterm bash -l"
  Shift+Alt + Return
